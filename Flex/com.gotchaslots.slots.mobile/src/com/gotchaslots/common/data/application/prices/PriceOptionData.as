package com.gotchaslots.common.data.application.prices
{
	import com.gotchaslots.common.utils.dataType.BaseIDData;
	
	public class PriceOptionData extends BaseIDData
	{
		// members
		private var _description:String;
		private var _productID:String;
		private var _chips:Number;
		
		// properties
		public function get Description():String
		{
			return _description;
		}
		public function get ProductID():String
		{
			return _productID;
		}
		public function get Chips():Number
		{
			return _chips;
		}
		
		// class
		public function PriceOptionData(id:int, description:String, productID:String, chips:Number)
		{
			super(id);
			
			_description = description;
			_productID = productID;
			_chips = chips;
		}
	}
}