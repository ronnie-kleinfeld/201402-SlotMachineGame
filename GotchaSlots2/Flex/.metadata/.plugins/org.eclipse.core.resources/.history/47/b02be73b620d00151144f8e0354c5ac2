		{
			super();
			
			Init();
		}
		public function Init():Boolean
		{
			throw new MustOverrideError();
		}
		
		// banner
		public function ShowBanner():void
		{
			throw new MustOverrideError();
		}
		
		// banner
		public function ShowInterstital():void
		{
			throw new MustOverrideError();
		}
	}
}