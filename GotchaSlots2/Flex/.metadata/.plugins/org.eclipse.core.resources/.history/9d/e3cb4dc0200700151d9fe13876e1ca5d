package com.gotchaslots.slots.mobile.utils.dataType
{
	import com.gotchaslots.slots.mobile.utils.ex.EventDispatcherEx;

	public class BaseIDData extends EventDispatcherEx
	{
		// members
		private var _id:int;
		
		// properties
		public function get ID():int
		{
			return _id;
		}
		
		// class
		public function BaseIDData(id:int)
		{
			super();
			
			_id = id;
		}
	}
}